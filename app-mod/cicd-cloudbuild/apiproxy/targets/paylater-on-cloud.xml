<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="paylater-on-cloud">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM.RemoveAPIKeyParam</Name>
            </Step>
            <Step>
                <Name>AM.Drop-Path-Suffix</Name>
            </Step>
            <Step>
                <Name>AM.SetTargetURL</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://paylater-core-dot-apigee-paylater.appspot.com/v1/paylater</URL>
    </HTTPTargetConnection>
</TargetEndpoint>